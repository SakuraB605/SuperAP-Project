package home.sj;

import java.util.ArrayList;

public class Multiples {
  private int height;
  private int width;

  Multiples(int h, int w){
    height = h;
    width = w;
  }
  public int getHeight() {
    return height;
  }
  public int getWidth() {
    return width;
  }
  public static ArrayList<Multiples> getMultiples(int num) {
	  ArrayList<Multiples> result = new ArrayList<>();
	  for (int i = 1; i <= num; i++) {
	    if (num % i == 0) {
	      result.add(new Multiples(i, num / i));
	    }
	  }
	  return result;
	}
} 
