package home.sj;

import java.awt.Color;
import java.util.ArrayList;

public class Square {
  private int num;
  private Color color;
  private ArrayList<Permutations> arr;
  private boolean isLocked;
/*Square(int number, Color color){
    num = number;
    this.color = color;
  }
*/
  Square(int number, ArrayList<Permutations> array){
    arr = new ArrayList<Permutations>();
    num = number;
    if (array != null) {      
      for (int i = 0; i < array.size(); i++) {
        arr.add(array.get(i));
      }
    }
  }
  Square(int number, ArrayList<Permutations> array, boolean isLocked){
	    arr = new ArrayList<Permutations>();
	    num = number;
	    if (array != null) {      
	      for (int i = 0; i < array.size(); i++) {
	        arr.add(array.get(i));
	      }
	    }
	    this.isLocked = isLocked;
  }
  Square(boolean isLocked){
    this.isLocked = isLocked;
  } 
  public boolean isLocked() {
	    return isLocked;
  }
  public int getNumber() {
    return num; 
  }
  public ArrayList<Permutations> getArrayList() {
    return arr;
  }
  public boolean getIsLocked() {
    return isLocked;
  }
}
