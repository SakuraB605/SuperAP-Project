package home.sj;

import java.awt.Color;
import java.util.ArrayList;

public class Square {
  int num;
  Color color;
  ArrayList<Permutations> arr;
  boolean isLocked;
  
/*Square(int number, Color color){
    num = number;
    this.color = color;
  }
*/
  Square(int number, ArrayList<Permutations> array, boolean isLocked){  
    arr = new ArrayList<Permutations>();
    num = number;
    if (array != null) {      
      for (int i = 0; i < array.size(); i++) {
        arr.add(array.get(i));
      }
    }
    isLocked = false;
  }
  Square(boolean isLocked){  
	    arr = null;
//	    num = null;
	    isLocked = false;
	  }
  public int getNumber() {
    return num; 
  }
  public ArrayList<Permutations> getArrayList() {
    return arr;
  }
}
