package home.sj;

public class Permutations {
	private int verticalShift;
	private int horizontalShift;
	private int height;
	private int width;
	  
	  Permutations(int vert , int hor, int height, int width){
	    verticalShift = vert;
	    horizontalShift = hor;
	    this.height = height;
	    this.width = width;
	  }
	  public int getHeight() {
	    return height;
	  }
	  public int getWidth() {
	    return width;
	  }
	  public int getVO() {
	    return verticalShift;
	  }
	  public int getHO() {
	    return horizontalShift;
	  }
	  
	  public String toString() {
	    return "Height: " + height + ", Width: " + width + ", VO: " + verticalShift + ", HO: " + horizontalShift;
	  }
	}
